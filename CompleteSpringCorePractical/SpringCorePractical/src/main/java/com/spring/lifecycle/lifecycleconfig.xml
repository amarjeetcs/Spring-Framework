<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-4.3.xsd 
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-4.3.xsd">
	<context:annotation-config />



	<bean class="com.spring.lifecycle.Samosa" name="s1"
		init-method="init" destroy-method="destroy">
		<property name="price" value="20.0" />
	</bean>

	<bean class="com.spring.lifecycle.Pepsi" name="p1">
		<property name="price" value="50.0" />
	</bean>

	<bean class="com.spring.lifecycle.Example" name="e1">
		<property name="subject" value="math" />
	</bean>
</beans>