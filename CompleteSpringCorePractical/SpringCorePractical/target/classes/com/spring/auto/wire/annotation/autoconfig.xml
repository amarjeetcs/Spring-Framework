<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-4.3.xsd 
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-4.3.xsd">
	<context:annotation-config />

	<bean class="com.spring.auto.wire.annotation.Address" name="temp1">
		<property name="street" value="turki"></property>
		<property name="city" value="bihar"></property>
	</bean>
	
	<bean class="com.spring.auto.wire.annotation.Address" name="temp2">
		<property name="street" value="kudhani"></property>
		<property name="city" value="bihar"></property>
	</bean>


	<!-- <bean class="com.spring.auto.wire.Emp" name="emp1" autowire="byName"/> -->
	<!-- <bean class="com.spring.auto.wire.Emp" name="emp1" autowire="byType"/> -->
	<bean class="com.spring.auto.wire.annotation.Emp" name="emp1"></bean>

</beans>